@using Microsoft.JSInterop
@page "/counter"
<h1>Counter</h1>

<p>Current count: @currentCount</p>

<button class="btn btn-primary" onclick="@IncrementCount">Click me</button>

@functions {

  protected override async Task OnInitAsync()
  {
    await JSRuntime.Current.InvokeAsync<string>("betterSeo.metaChanger", "Counter", "Counter demo description", "FetchData,blazor,serverside");
  }

  int currentCount = 0;

  void IncrementCount()
  {
    currentCount++;
  }

}